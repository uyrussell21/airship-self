{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src"],
      "@components/*": ["src/components/*", "src/components"],
      "@utils/*": ["src/utils/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"]
      // "@theme": ["src/styles/theme"]
    },
    "module": "commonjs",
    "target": "es6"
  },
  "exclude": ["node_modules"]
}
